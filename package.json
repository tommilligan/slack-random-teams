{
  "name": "slack-random-teams",
  "version": "0.1.0",
  "description": "Split a slack channel into random teams",
  "main": "src/server.js",
  "repository": "git@github.com:tommilligan/slack-random-teams",
  "author": "Tom Milligan <code@tommilligan.net>",
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@slack/client": "^3.16.0",
    "body-parser": "^1.18.2",
    "dotenv-safe": "^4.0.4",
    "express": "^4.16.2",
    "readable-id": "^1.0.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^4.18.0",
    "eslint-plugin-mocha": "^4.11.0",
    "flow-bin": "^0.66.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.1",
    "mocha-eslint": "^4.1.0",
    "nodemon": "^1.15.0",
    "nyc": "^11.4.1"
  },
  "scripts": {
    "start": "node dist/index.js",
    "type": "yarn flow",
    "test": "cross-env NODE_ENV=test nyc mocha -R spec test/",
    "compile": "babel src -d dist",
    "dev": "nodemon -w src --exec \"babel-node src/index.js\"",
    "prepublish": "npm run build",
    "build": "yarn flow && yarn compile && yarn coverage"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
